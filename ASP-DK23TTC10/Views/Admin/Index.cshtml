@model List<ASP_DK23TTC10.Models.admin>

@{
    ViewBag.Title = "Quản lý tài khoản Admin";
    Layout = "~/Views/Shared/_LayoutPage1.cshtml";
}

<h2>Quản lý tài khoản Admin</h2>
<div class="row">
    <div class="col-2"></div>
    <div class="col-8">
        <table class="table table-striped table-hover">
            <thead>
                <tr>
                    <th scope="col">STT</th>
                    <th scope="col">Email</th>
                    <th scope="col">Tên Admin</th>
                    <th scope="col">Số điện thoại</th>
                    <th scope="col">Avatar</th>
                    <th scope="col">Phân quyền</th>
                    <th scope="col">Thời gian cập nhật cuối</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <th scope="row">@item.id</th>
                        <td>@item.email</td>
                        <td>@item.name</td>
                        <td>@item.phone</td>              
                        <td>@item.avatar</td>
                        <td>@item.role</td>                
                        <td>@item.created_at</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
    <div class="col-2"></div>
</div>
